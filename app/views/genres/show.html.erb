<h1><%= @genre.name %></h1>
<% if !@genre.songs.empty? %>
  <h3>Songs:</h3>
  <% @genre.songs.each do |song| %>
  <ul>
    <li><%= link_to song.name, song_path(song.id) %></li>
  </ul>
  <%end%>
<% end %>
<% if !@genre.artists.empty? %>
  <h3>Artists</h3>
  <% @genre.artists.each do |artist| %>
  <ul>
    <li><%= link_to artist.name, artist_path(artist.id) %></li>
  </ul>
  <%end%>
<%end%>
