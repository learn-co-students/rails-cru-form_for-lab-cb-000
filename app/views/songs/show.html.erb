<h1>Song Info:</h1>
<h1>Name: <%= @song.name %></h1>
<h2>Artist: <%= link_to @song.artist.name, artist_path(@song.artist_id) %></h2>
<h3>Genre: <%= link_to @song.genre.name, genre_path(@song.genre_id) %></h3>

<%= form_tag edit_song_path(@song), method: "get" do %>
  <%= submit_tag "Edit Song" %>
<% end %>
<br>
<br>
<%= form_tag artists_path, method: "get" do %>
  <%= submit_tag "List Artists" %>
<% end %>

<%= form_tag songs_path, method: "get" do %>
  <%= submit_tag "List Songs" %>
<% end %>

<%= form_tag genres_path, method: "get" do %>
  <%= submit_tag "List Genres" %>
<% end %>
