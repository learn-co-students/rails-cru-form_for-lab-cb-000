<!DOCTYPE html>
<html>
    <head>
        <title>Edit <%= @artist.name %></title>
    </head>
    <body>
        <%= form_for(@artist) do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>
            <br>
            <%= f.label :bio %>
            <%= f.text_area :bio %>
            <br>
            <%= f.submit %>
        <% end %>
    </body>
</html>