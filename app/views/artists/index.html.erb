<h1>Listing Artists</h1>

<table>
  <div>
    <tr>
      <th>Name</th>
      <th>Bio</th>
    <tr>
  </div>

  <% @artists.each do |artist| %>
    <div>
      <tr>
        <td><%= link_to artist.name, artist_path(artist) %>
        <td><%= artist.bio %>
      </tr>
    </div>
  <% end %>
</table>

<br>

<%#= link_to "New artist", new_artist_path %>
<%= form_tag new_artist_path, method: :get do %>
  <div><%= submit_tag "New artist" %></div>
<% end %>
